{
  "name": "@stiforr/recipe-data-scraper",
  "version": "1.4.1",
  "description": "web scraper for recipe data",
  "repository": "github:thelifenadine/recipe-data-scraper",
  "main": "dist/index.js",
  "source": "src/main.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "watch": "parcel watch",
    "parcel:build": "parcel build",
    "lint": "eslint './src/**/*.ts'",
    "test": "cross-env NODE_ENV=test ts-mocha './src/**/*.test.ts' --exit",
    "coveralls": "cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov",
    "print-recipe-tester": "cross-env LOGGING_ENABLED=true babel-node ./scripts/printRecipeTester",
    "test-url": "cross-env LOGGING_ENABLED=true babel-node ./scripts/urlTesterCLI"
  },
  "author": "Nadine Kinnen",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "0.25.0",
    "cheerio": "1.0.0-rc.10",
    "iso8601-duration": "1.3.0",
    "microdata-node": "2.0.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.7.0",
    "@parcel/transformer-typescript-types": "2.7.0",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "@types/proxyquire": "^1.3.28",
    "@types/readline-sync": "^1.4.4",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "chai": "4.3.6",
    "coveralls": "3.1.1",
    "cross-env": "7.0.3",
    "eslint": "8.22.0",
    "esm": "^3.2.25",
    "istanbul": "0.4.5",
    "mocha": "10.0.0",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "15.1.0",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "proxyquire": "2.1.3",
    "readline-sync": "1.4.10",
    "sinon": "12.0.1",
    "ts-migrate": "^0.1.33",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "volta": {
    "node": "18.9.0",
    "yarn": "1.22.19"
  },
  "prettier": {
    "printWidth": 100
  }
}
